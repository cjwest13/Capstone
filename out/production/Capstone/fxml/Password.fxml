<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<AnchorPane stylesheets="@../styles/password.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project.PasswordController">
   <children>
      <BorderPane fx:id="border" stylesheets="@../styles/password.css" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <center>
            <GridPane prefHeight="333.0" prefWidth="600.0" stylesheets="@../styles/password.css" BorderPane.alignment="CENTER">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints maxHeight="107.0" minHeight="10.0" prefHeight="106.0" vgrow="SOMETIMES" />
                  <RowConstraints maxHeight="103.0" minHeight="10.0" prefHeight="42.0" vgrow="SOMETIMES" />
                <RowConstraints maxHeight="118.0" minHeight="10.0" prefHeight="23.0" vgrow="SOMETIMES" />
                <RowConstraints maxHeight="178.0" minHeight="10.0" prefHeight="162.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Button fx:id="submit" mnemonicParsing="false" onAction="#changePassword" prefHeight="42.0" prefWidth="110.0" stylesheets="@../styles/password.css" text="Submit" GridPane.halignment="CENTER" GridPane.rowIndex="3" GridPane.valignment="TOP" />
                  <Label stylesheets="@../styles/password.css" text="Enter Password" GridPane.halignment="CENTER" GridPane.valignment="BOTTOM" />
                  <PasswordField fx:id="password" maxWidth="-Infinity" prefHeight="34.0" prefWidth="209.0" stylesheets="@../styles/password.css" GridPane.halignment="CENTER" GridPane.rowIndex="1" />
               </children>
            </GridPane>
         </center>
         <bottom>
            <GridPane fx:id="btmGrid" prefHeight="40.0" prefWidth="600.0" stylesheets="@../styles/password.css" BorderPane.alignment="CENTER">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Button fx:id="backbtn" mnemonicParsing="false" onAction="#goBack" prefHeight="25.0" prefWidth="72.0" stylesheets="@../styles/password.css" text="Back" GridPane.halignment="LEFT" />
                  <Label fx:id="timeLbl" alignment="CENTER_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="17.0" prefWidth="133.0" stylesheets="@../styles/password.css" text="Time, Date" textAlignment="RIGHT" GridPane.columnIndex="1" GridPane.halignment="RIGHT" />
               </children>
            </GridPane>
         </bottom>
      </BorderPane>
   </children>
</AnchorPane>
